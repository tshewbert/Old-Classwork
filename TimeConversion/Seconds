import java.util.Scanner;

public class TimeConversion
{
		public static void main(String[] args)
		{
			
			//User inputed data
			int userInput;
			
			//Get the number of seconds from the user
			Scanner keyboard = new Scanner(System.in);
			System.out.println("Enter number of seconds:");
			userInput = keyboard.nextInt();
			if (userInput <= 0)
			else(
				System.out.println("The number of seconds entered may not be less than zero.+"
						+ "/nNo conversion was performed.");)
			if else (user input <= 59)
			else (System.out.println(userInput+ "seconds is less than 1 minute.  No conversion was performed."))
		}
}